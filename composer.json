{
    "name": "santoshjena/livewire-range-slider",
    "description": "A Tall Stack wrapper for noUiSlider Range Slider",
    "keywords": [
        "santoshjena",
        "livewire-range-slider"
    ],
    "homepage": "https://github.com/santoshjena007/livewire-range-slider",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Santosh Jena",
            "email": "santoshk.jena@hyscaler.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.2",
        "illuminate/support": "^8.67|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.14|^9.0",
        "phpunit/phpunit": "^10.4|^11.1",
        "livewire/livewire": "^3.1|^3.4"
    },
    "autoload": {
        "psr-4": {
            "SantoshJena\\LivewireRangeSlider\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SantoshJena\\LivewireRangeSlider\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SantoshJena\\LivewireRangeSlider\\LivewireRangeSliderServiceProvider"
            ],
            "aliases": {
                "LivewireRangeSlider": "SantoshJena\\LivewireRangeSlider\\LivewireRangeSliderFacade"
            }
        }
    }
}